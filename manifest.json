{
  "name": "ChatGPT",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "chat.html"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
      "*://*/*"
  ]
}